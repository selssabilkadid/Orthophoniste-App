<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Layouts.GradeTestExerciceController"
            fx:id="rootLayout"  prefHeight="400.0" prefWidth="600.0">
    <VBox layoutX="20.0" layoutY="20.0" prefWidth="560.0" spacing="10.0">
