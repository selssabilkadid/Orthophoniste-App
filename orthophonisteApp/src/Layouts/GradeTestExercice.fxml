        <VBox fx:id="exerciceContainer" spacing="10.0" />
    </VBox>
    <Button fx:id="savescorebtn" layoutX="274.0" layoutY="343.0" mnemonicParsing="false" onAction="#saveScore" text="Save score" />
</AnchorPane>
